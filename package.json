{
  "name": "optery-gpc-chrome-extension",
  "version": "0.0.3",
  "private": false,
  "license": "MIT",
  "workspaces": [
    "chrome",
    "firefox"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:chrome": "npm run build --workspace=chrome",
    "build:firefox": "npm run build --workspace=firefox",
    "start": "npm run start --workspaces",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "tailwindcss": "^3.4.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "nth-check": "^2.1.1",
    "postcss": ">=8.4.31",
    "ws": ">=8.17.1"
  },
  "overrides": {
    "nth-check": "$nth-check",
    "postcss": "$postcss",
    "ws": "$ws"
  }
}
